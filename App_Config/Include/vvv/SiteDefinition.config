<?xml version="1.0" encoding="utf-8" ?>
<!--

-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<sites>
			<site name="Eurtechclone" patch:before="site[@name='website']"
				  hostName ="eurtechclone.local"
				  virtualFolder="/"
				  physicalFolder="/"
				  rootPath="/sitecore/content/"
				  startItem="/home/data/Eurtech/"
				  database="master"
				  domain="extranet"
				  allowDebug="true"
				  cacheHtml="true"
				  htmlCacheSize="50MB"
				  enablePreview="true"
				  enableWebEdit="true"
				  enableDebugger="true"
				  disableClientData="false"/>
		</sites>
	</sitecore>
</configuration>